{
  "name": "pmnps-workspace",
  "private": false,
  "projectType": "monorepo",
  "useGit": true,
  "usePerformanceFirst": false,
  "registry": "https://registry.npmmirror.com",
  "plugins": [
    "@pmnps/plugin-vscode-workspace",
    "@pmnps/plugin-fork",
    "@pmnps/plugin-clean-node-modules",
    [
      "@pmnps/plugin-publish",
      {
        "registry": "https://registry.npmjs.org"
      }
    ]
  ],
  "useCommandHelp": true,
  "core": "npm",
  "forbiddenWorkspacePackageInstall": false,
  "refreshAfterInstall": false,
  "listPackageDependencies": false,
  "npmCiFirst": true
}