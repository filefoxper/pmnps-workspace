{
  "description": "This is a typescript build plugin for pmnps",
  "version": "3.1.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/filefoxper/pmnps-workspace/packages/@pmnps/plugin-typescript"
  },
  "homepage": "https://github.com/filefoxper/pmnps-workspace/packages/@pmnps/plugin-typescript",
  "author": "Jimmy.Harding",
  "dependencies": {
    "@pmnps/core": ">=3.1.9"
  },
  "devDependencies": {
    "prettier": "^2.7.0",
    "typescript": "4.5.5"
  },
  "main": "dist/index.js",
  "module": "src/index.ts",
  "typings": "index.d.ts",
  "files": [
    "index.d.ts",
    "dist"
  ],
  "name": "@pmnps/plugin-typescript",
  "private": false,
  "scripts": {
    "build": "esbuild ./src/index.ts --outfile=./dist/index.js --bundle --target=node17 --platform=node --format=cjs"
  }
}
